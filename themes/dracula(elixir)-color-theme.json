{
	"name": "Dracula Elixir",
	"type": "dark",
	"include": "./dracula.json",
	"tokenColors": [
		{
			"name": "Atoms",
			"scope": [
					"constant.other.symbol.elixir",
					"constant.language.symbol.elixir"
			],
			"settings": {
					"foreground": "#8BE9FD",
			}
		},
		{
			"name": "Atom colons",
			"scope": [
					"punctuation.definition.constant.elixir"
			],
			"settings": {
					"foreground": "#8BE9FD"
			}
		},
		{
			"name": "Numbers",
			"scope": [
					"constant.numeric.elixir",
			],
			"settings": {
					"foreground": "#BD93F9",
			}
		},
		{
			"name": "Non-numeric constants",
			"scope": [
					"constant.language.elixir"
			],
			"settings": {
					"foreground": "#BD93F9"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
					"source.elixir.embedded.source"
			],
			"settings": {
					"fontStyle": "italic",
					"foreground": "#9977c9"
			}
		},
		{
			"name": "Key word operators",
			"scope": [
					"keyword.operator.other.elixir",
					"keyword.operator.arithmetic.elixir",
					"keyword.operator.logical.elixir",
					"keyword.operator.assignment.elixir",
					"keyword.operator.comparison.elixir",
					"keyword.operator.elixir"
			],
			"settings": {
					"foreground": "#FF79C6"
			}
		},
		{
			"name": "Comments",
			"scope": [
					"comment.line.number-sign.elixir",
					"comment.documentation.heredoc.elixir"
			],
			"settings": {
					"foreground": "#6272A4"
			}
		},
		{
			"name": "Wild cards and unused args",
			"scope": [
					"wildcard.comment.elixir",
					"unused.comment.elixir"
			],
			"settings": {
					"fontStyle": "italic",
					"foreground": "#6272A4"
			}
		},
		{
			"name": "Module variables",
			"scope": [
					"variable.other.readwrite.module.elixir"
			],
			"settings": {
					"foreground": "#FFB86C"
			}
		},
		{
			"name": "Modules",
			"scope": [
					"variable.other.constant.elixir"
			],
			"settings": {
					"foreground": "#BD93F9"
			}
		},
		{
			"name": "Strings",
			"scope": [
					"string.quoted.double.elixir",
					"string.quoted.double.heredoc.elixir",
					"string.quoted.double.interpolated.elixir",
					"string.regexp.interpolated.elixir",
					"string.regexp.group.elixir",
					"string.regexp.character-class.elixir"
			],
			"settings": {
					"foreground": "#F1FA8C"
			}
		},
		{
			"name": "Lambda captures",
			"scope": [
					"variable.other.anonymous.elixir"
			],
			"settings": {
					"foreground": "#FFB86C"
			}
		},
		{
			"name": "Escapes",
			"scope": [
					"constant.character.escape.elixir"
			],
			"settings": {
					"foreground": "#FFB86C"
			}
		},
		{
			"name": "Regexp classes",
			"scope": [
					"punctuation.definition.character-class.elixir"
			],
			"settings": {
					"foreground": "#8BE9FD"
			}
		},
		{
			"name": "Regexp groups",
			"scope": [
					"punctuation.definition.group.elixir"
			],
			"settings": {
					"foreground": "#FF5555"
			}
		},
		{
			"name": "Sigil literals",
			"scope": [
					"string.quoted.double.literal.elixir"
			],
			"settings": {
					"foreground": "#FFB86C"
			}
		},
		{
			"name": "Html embedded tags",
			"scope": [
					"punctuation.section.embedded.elixir"
			],
			"settings": {
					"foreground": "#BD93F9"
			}
		}
	]
}
